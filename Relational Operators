#include <stdio.h> 
#include <conio.h>
 
void  main() 
{ 
clrscr();
	int n;

	printf( "\n Enter any Number : " );
	scanf( "%d", &n );

	printf( "\n (n < 10) is = %d", (n < 10) );
	printf( "\n (n > 10) is = %d", (n > 10) );
	printf( "\n (n <= 10) is = %d", (n <= 10) );
	printf( "\n (n >= 10) is = %d", (n >= 10) );
	printf( "\n (n == 10) is = %d", (n == 10) );
	printf( "\n (n != 10) is = %d", (n != 10) );

	getch();
}
